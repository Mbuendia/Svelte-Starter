{
  "name": "Frontend-Starter-Kit",
  "version": "1.0.0",
  "description": "A boilerplate for HTML5, Material, Firebase, Gulp, Rollup, Babel, Reshape, and PostCSS.",
  "scripts": {
    "start": "gulp --watch --serve",
    "test": "gulp unit --watch",
    "dev": "gulp --watch --serve",
    "prod": "gulp --prod",
    "lint": "gulp lint",
    "unit": "gulp unit",
    "e2e": "gulp e2e",
    "reset": "git reset --hard HEAD && git clean -df",
    "reinstall": "yarn cache clean && git clean -dfx -e '.*' && yarn install",
    "deploy": "firebase deploy",
    "gulp": "gulp",
    "predev": "rimraf public",
    "preprod": "rimraf public",
    "postinstall": "gulp webdriver && cd functions && yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Frontend-Starter-Kit.git"
  },
  "keywords": [
    "HTML5"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Frontend-Starter-Kit/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Frontend-Starter-Kit#readme",
