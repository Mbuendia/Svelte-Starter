{
  "name": "Vanilla-Starter-Kit",
  "version": "1.0.0",
  "description": "A boilerplate for vanilla HTML/CSS/JS, Gulp, and Rollup.",
  "scripts": {
    "start": "gulp",
    "dev": "rimraf dist && gulp",
    "dev-watch": "echo \"TODO\"",
    "test": "karma start",
    "test-watch": "karma start --no-single-run",
    "prod": "rimraf dist && gulp build",
    "e2e": "gulp e2e",
    "webdriver": "gulp webdriver",
    "clean": "rimraf dist",
    "reset": "git reset --hard HEAD && git clean -df",
    "reinstall": "yarn cache clean && git clean -dfx -e '.*' && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Vanilla-Starter-Kit.git"
  },
  "keywords": [
    "Vanilla"
  ],
  "author": "陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Vanilla-Starter-Kit/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Vanilla-Starter-Kit#readme",
  "dependencies": {
    "chart.js": "^2.3.0",
    "core-js": "^2.4.1",
    "firebase": "^3.5.2",
    "material-design-lite": "^1.2.1",
    "page": "^1.7.1"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "browser-sync": "^2.17.5",
    "connect-history-api-fallback": "^1.3.0",
    "cssnano": "^3.8.0",
    "express": "^4.14.0",
    "express-history-api-fallback": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-changed": "^1.3.2",
    "gulp-htmlmin": "^3.0.0",
    "gulp-imagemin": "^3.0.3",
    "gulp-newer": "^1.3.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-protractor": "^3.0.0",
    "imagemin-pngquant": "^5.0.0",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^2.7.0",
    "karma": "^1.3.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-rollup-plugin": "^0.2.4",
    "phantomjs-prebuilt": "^2.1.13",
    "postcss-comment": "^1.0.3",
