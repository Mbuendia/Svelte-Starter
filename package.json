{
  "name": "Web-Starter-Kit",
  "version": "1.0.0",
  "description": "A boilerplate for Lodash, RxJS, Redux, Material, Firebase, and Chart.js.",
  "scripts": {
    "start": "gulp dev --mode dev",
    "dev": "rimraf public && gulp dev --mode dev",
    "dev-watch": "rimraf public && gulp dev --mode dev --serve off",
    "test": "gulp unit",
    "test-watch": "gulp unit --watch true",
    "prod": "rimraf public && gulp build --mode prod",
    "e2e": "gulp e2e",
    "lint": "gulp lint",
    "webdriver": "gulp webdriver",
    "clean": "rimraf public",
    "reset": "git reset --hard HEAD && git clean -df",
    "reinstall": "yarn cache clean && git clean -dfx -e '.*' && yarn install",
    "deploy": "firebase deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Web-Starter-Kit.git"
  },
  "keywords": [
    "Vanilla"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Web-Starter-Kit/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Web-Starter-Kit#readme",
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "dependencies": {
    "@reactivex/rxjs": "5.0.2",
    "chart.js": "2.4.0",
    "core-js": "2.4.1",
    "d3": "4.4.1",
    "dialog-polyfill": "0.4.5",
    "firebase": "3.6.3",
    "intl": "1.2.5",
    "lodash-es": "4.17.4",
    "luyou": "0.1.0",
    "material-design-lite": "1.3.0",
    "redux": "3.6.0",
    "roll-redux-observable": "0.3.0",
    "rxjs": "5.0.2"
  },
  "devDependencies": {
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-plugin-external-helpers": "6.18.0",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-transform-function-bind": "6.8.0",
    "babel-preset-latest": "6.16.0",
    "browser-sync": "2.18.6",
    "connect-history-api-fallback": "1.3.0",
    "cssnano": "3.10.0",
    "express": "4.14.0",
    "express-history-api-fallback": "2.1.0",
    "firebase-tools": "3.2.1",
    "gulp": "3.9.1",
    "gulp-eslint": "3.0.1",
    "gulp-htmlhint": "0.3.1",
    "gulp-htmlmin": "3.0.0",
    "gulp-notify": "2.2.0",
    "gulp-plumber": "1.1.0",
    "gulp-protractor": "3.0.0",
    "gulp-sourcemaps": "2.4.0",
    "gulp-stylelint": "3.7.0",
    "gulp-util": "3.0.8",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.1.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.1",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-rollup-plugin": "0.2.4",
    "phantomjs-prebuilt": "2.1.14",
    "postcss-comment": "1.0.3",
    "postcss-cssnext": "2.9.0",
