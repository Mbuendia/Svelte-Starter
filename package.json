    "rollup-plugin-postcss": "0.2.0",
    "rollup-plugin-replace": "1.1.1",
    "rollup-plugin-uglify": "1.0.1",
    "rollup-stream": "1.18.0",
    "rucksack-css": "0.9.1",
    "run-sequence": "1.2.2",
    "stylelint-config-standard": "15.0.1",
    "sw-precache": "4.3.0",
