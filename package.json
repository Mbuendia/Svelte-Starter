{
  "name": "Vanilla-Starter-Kit",
  "version": "1.0.0",
  "description": "A boilerplate for Vanilla, Lodash, ReactiveX, Material, Firebase, Gulp, Rollup, PostCSS, and Babel.",
  "scripts": {
    "start": "gulp dev --type dev",
    "dev": "rimraf public && gulp dev --type dev",
    "dev-watch": "rimraf public && gulp dev-watch --type dev",
    "test": "gulp unit",
    "test-watch": "gulp unit --type watch",
    "prod": "rimraf public && gulp build --type prod",
    "e2e": "gulp e2e",
    "lint": "gulp lint",
    "webdriver": "gulp webdriver",
    "clean": "rimraf public",
    "reset": "git reset --hard HEAD && git clean -df",
    "reinstall": "yarn cache clean && git clean -dfx -e '.*' && yarn install",
    "deploy": "firebase deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Vanilla-Starter-Kit.git"
  },
  "keywords": [
    "Vanilla"
  ],
  "author": "陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Vanilla-Starter-Kit/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Vanilla-Starter-Kit#readme",
  "dependencies": {
    "chart.js": "^2.4.0",
    "core-js": "^2.4.1",
    "dialog-polyfill": "^0.4.5",
    "firebase": "^3.6.1",
    "intl": "^1.2.5",
    "lodash-es": "^4.17.2",
    "material-design-lite": "^1.2.1",
    "page": "^1.7.1",
    "rxjs": "^5.0.1",
    "universal-router": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "browser-sync": "^2.18.2",
    "connect-history-api-fallback": "^1.3.0",
    "cssnano": "^3.8.1",
    "express": "^4.14.0",
    "express-history-api-fallback": "^2.0.0",
    "firebase-tools": "^3.2.0",
    "gulp": "^3.9.1",
    "gulp-changed": "^1.3.2",
    "gulp-eslint": "^3.0.1",
    "gulp-htmlhint": "^0.3.1",
    "gulp-htmlmin": "^3.0.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-protractor": "^3.0.0",
    "gulp-sourcemaps": "^2.2.1",
    "gulp-stylelint": "^3.4.0",
    "gulp-util": "^3.0.7",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^2.7.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-rollup-plugin": "^0.2.4",
    "phantomjs-prebuilt": "^2.1.13",
    "postcss-comment": "^1.0.3",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^9.0.0",
    "postcss-modules": "^0.5.2",
    "protractor": "^4.0.12",
    "require-dir": "^0.3.1",
    "rimraf": "^2.5.4",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-image": "^1.0.2",
    "rollup-plugin-json": "^2.0.2",
    "rollup-plugin-node-builtins": "^2.0.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-postcss": "^0.2.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-stream": "^1.14.0",
    "rucksack-css": "^0.9.1",
    "run-sequence": "^1.2.2",
    "stylelint-config-standard": "^15.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "webdriver-manager": "^10.2.10"
  }
}
