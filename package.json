{
  "name": "Web-Starter-Kit",
  "version": "1.0.0",
  "description": "A boilerplate for HTML5, Material, Firebase, Gulp, Rollup, Babel, PostHTML, and PostCSS.",
  "scripts": {
    "start": "gulp --watch --serve",
    "test": "gulp unit --watch",
    "dev": "gulp --watch --serve",
    "prod": "gulp --prod",
    "lint": "gulp lint",
    "unit": "gulp unit",
    "e2e": "gulp e2e",
    "clean": "rimraf public",
    "reset": "git reset --hard HEAD && git clean -df",
    "reinstall": "yarn cache clean && git clean -dfx -e '.*' && yarn install",
    "deploy": "firebase deploy",
    "gulp": "gulp",
    "flow": "flow",
    "predev": "rimraf public",
    "preprod": "rimraf public",
    "postinstall": "gulp webdriver && cd functions && yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Web-Starter-Kit.git"
  },
  "keywords": [
    "HTML5"
  ],
  "author": "Yen-cheng Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/Web-Starter-Kit/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/Web-Starter-Kit#readme",
