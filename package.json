{
  "name": "Vanilla-Starter-Kit",
  "version": "1.0.0",
  "description": "A boilerplate for Vanilla, Material, and Firebase.",
  "scripts": {
    "start": "gulp",
    "test": "karma start",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "dev": "rimraf dist && gulp",
    "prod": "rimraf dist && gulp build",
    "clean": "rimraf dist",
    "reset": "git reset --hard HEAD && git clean -df",
    "reinstall": "yarn cache clean && git clean -dfx -e '.*' && yarn install"
  },
  "keywords": [
    "Vanilla",
    "Material",
    "Firebase"
  ],
  "author": "陳彥澄",
  "license": "MIT",
  "dependencies": {
    "core-js": "^2.4.1",
    "firebase": "^3.5.2",
    "material-design-lite": "^1.2.1",
    "page": "^1.7.1"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-exponentiation-operator": "^6.8.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "browser-sync": "^2.17.5",
    "connect-history-api-fallback": "^1.3.0",
    "cssnano": "^3.8.0",
    "gulp": "^3.9.1",
    "gulp-changed": "^1.3.2",
    "gulp-htmlmin": "^3.0.0",
    "gulp-imagemin": "^3.0.3",
    "gulp-newer": "^1.3.0",
    "gulp-plumber": "^1.1.0",
    "imagemin-pngquant": "^5.0.0",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^2.7.0",
    "karma": "^1.3.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "phantomjs-prebuilt": "^2.1.13",
    "postcss-comment": "^1.0.3",
    "postcss-cssnext": "^2.8.0",
    "protractor": "^4.0.10",
    "rimraf": "^2.5.4",
    "rollup-plugin-async": "^1.1.0",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-inject": "^2.0.0",
    "rollup-plugin-node-builtins": "^2.0.0",
    "rollup-plugin-node-globals": "^1.0.9",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-postcss": "^0.2.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-stream": "^1.14.0",
    "rucksack-css": "^0.8.6",
    "run-sequence": "^1.2.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
